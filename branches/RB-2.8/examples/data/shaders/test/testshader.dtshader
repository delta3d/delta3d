<?xml version="1.0" encoding="UTF-8"?>

<shaderlist>
   <shadergroup name="DemoShaders">
      <shader name="TestShader" default="yes" verticesout="3">
         <source type="Vertex">shaders/test/testshader.vert</source>
         <source type="Vertex">shaders/base/vertex_functions.vert</source>
         <source type="Fragment">shaders/test/testshader.frag</source>
         <source type="Fragment">shaders/base/fragment_functions.frag</source>
      </shader>
      
      <shader name="TestShaderWithoutGeomShader" default="no">
         <source type="Vertex">Shaders/test/testshader.vert</source>
         <source type="Fragment">Shaders/test/testshaderAlternate.frag</source>
      </shader>
   </shadergroup>
</shaderlist>
