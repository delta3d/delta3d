import os

Import('*')

srcs = env.SGlob( '*.cpp' )

deps=[ 'dtUtil', 'dtCore', 'osg', 'osgDB', 'opengl',
       'User32', 'Advapi32', 'Rpcrt4', 'Winmm', 'Gdi32',  
       'CoreFoundation', 'IOKit', 'Carbon' ]

defines = []

if env['OS'] == 'windows':
   defines = ['DT_AI_LIBRARY', 'NOMINMAX']

env.BuildLib( 'dtAI', srcs, deps, defines )
