Import('*')

dtAudioSrcs = [ 'audiomanager.cpp', 
		'listener.cpp', 
		'sound.cpp', 
		'soundeffectbinder.cpp' ]

env = env.Copy()

dtAudioLib = env.SharedLibrary( 'dtaudio', dtAudioSrcs, LIBS=[ 'openal', 'OpenThreads', 'osgDB', 'plibsg', 'plibul', 'Producer', 'tinyxml' ] )

env.Install( '../../lib', dtAudioLib )
